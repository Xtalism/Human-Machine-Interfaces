number_list = []

while True:
    user_input = (float(input('Enter a float number (or "q" to quit): ')))
    
    if user_input == -1.0:  # Check if the user wants to quit
        break
    
    try:
        number_list.append(user_input)  # Add to the list
    except ValueError:
        print("Invalid input. Please enter a valid float number or 'q' to quit.")

print("Your list of numbers:", number_list)